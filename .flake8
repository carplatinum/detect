[flake8]
max-line-length = 120
extend-ignore = E203, W503
exclude =
    .git,
    __pycache__,
    venv,
    env,
    .env,
    .venv,
    .pytest_cache,
    .mypy_cache,
    migrations,
    static,
    media,
    node_modules,
    *.pyc

max-complexity = 10
max-doc-length = 120

select = B,C,E,F,W,T4,B9
